<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300" prefWidth="400" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="GUI.AreYou.gameController">

    <Text layoutX="160" layoutY="100" text="Are You Gay?" />
    <Button fx:id="yesButton" layoutX="212.0" layoutY="156.0" onAction="#handleYes" text="Yes" />
    <Button fx:id="noButton" layoutX="144.0" layoutY="156.0" onMouseEntered="#handleNoButtonHover" text="No" />

</AnchorPane>
