<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AreYou.gameController"
            prefWidth="400" prefHeight="300">

    <Text layoutX="160" layoutY="100" text="Are you gay?" />
    <Button fx:id="yesButton" layoutX="150" layoutY="200" text="Yes" onAction="#handleYes" />
    <Button fx:id="noButton" layoutX="250" layoutY="200" text="No" onMouseEntered="#handleNoHover" />

</AnchorPane>
